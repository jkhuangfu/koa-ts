{
  "name": "koa-ts",
  "version": "1.0.0",
  "description": "使用typescript开发koa2",
  "main": "app.ts",
  "scriptsDescription": {
    "build": "打包",
    "copy": "复制非ts文件到打包目录",
    "clean": "清理打包目录",
    "dev": "本地数据环境开发启动",
    "debug": "连接生产数据进行启动",
    "lint": "tslint检验",
    "fix": "tslint修复"
  },
  "scripts": {
    "start": "pm2 start ./start.json",
    "build": "npm run lint && npm run clean && tsc && npm run copy && node ./build.js",
    "copy": "copyfiles package.json ./build start.json ./build yarn.lock ./build",
    "clean": "rimraf ./build",
    "dev": "cross-env NODE_ENV=development ts-node-dev --transpile-only src/app.ts",
    "debug": "cross-env NODE_ENV=production ts-node-dev --transpile-only src/app.ts",
    "lint": "tslint --project tsconfig.json --config tslint.json",
    "fix": "tslint --fix -c tslint.json ./src/**/*.ts"
  },
  "keywords": [
    "koa2",
    "typescript",
    "redis",
    "mysql",
    "session",
    "mongoose",
    "mongodb",
    "jsonwentoken",
    "log4js",
    "soccket"
  ],
  "author": "jkhuangfu",
  "license": "ISC",
  "dependencies": {
    "@koa/cors": "^3.0.0",
    "adm-zip": "^0.4.13",
    "ali-oss": "^6.11.2",
    "axios": "^0.21.0",
    "bull": "^3.18.1",
    "cheerio": "^1.0.0-rc.3",
    "chokidar": "^3.4.3",
    "copyfiles": "^2.4.0",
    "cross-env": "^7.0.2",
    "ejs": "^3.0.1",
    "jsonwebtoken": "^8.5.1",
    "koa": "^2.11.0",
    "koa-body": "^4.1.1",
    "koa-helmet": "^6.0.0",
    "koa-router": "^9.1.0",
    "koa-session": "^6.0.0",
    "koa-static": "^5.0.0",
    "koa-views": "^6.3.1",
    "log4js": "^6.1.0",
    "module-alias": "^2.2.2",
    "mongoose": "^5.10.15",
    "mysql": "^2.17.1",
    "nodemailer": "^6.4.16",
    "redis": "^3.0.2",
    "socket.io": "^2.3.0",
    "svg-captcha": "^1.4.0",
    "uuid": "^8.3.1",
    "websocket": "^1.0.31",
    "xml2js": "^0.4.23"
  },
  "devDependencies": {
    "@types/adm-zip": "^0.4.32",
    "@types/ali-oss": "^6.0.5",
    "@types/bull": "^3.14.4",
    "@types/cheerio": "^0.22.22",
    "@types/ejs": "^3.0.5",
    "@types/jsonwebtoken": "^8.3.5",
    "@types/koa-helmet": "^6.0.1",
    "@types/koa-router": "^7.0.42",
    "@types/koa-session": "^5.10.1",
    "@types/koa-static": "^4.0.1",
    "@types/koa-views": "^2.0.4",
    "@types/koa__cors": "^3.0.2",
    "@types/module-alias": "^2.0.0",
    "@types/mongoose": "^5.10.1",
    "@types/mysql": "^2.15.8",
    "@types/node": "^14.14.8",
    "@types/nodemailer": "^6.4.0",
    "@types/redis": "^2.8.28",
    "@types/socket.io": "^2.1.8",
    "@types/uuid": "^8.0.0",
    "@types/websocket": "^1.0.1",
    "@types/xml2js": "^0.4.7",
    "rimraf": "^3.0.2",
    "ts-node-dev": "^1.0.0",
    "tslint": "^6.1.2",
    "typescript": "^4.0.5"
  },
  "_moduleAliases": {
    "@": "src"
  }
}
